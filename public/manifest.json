{
    "manifest_version": 3,
    "name": "elfur",
    "version": "0.1.0",
    "description": "elfur Style Extension",
    "action": {
        "default_popup": "index.html",
        "default_title": "Launch elfur Style Tool"
    },
    "host_permissions": [
        "https://www.linkedin.com/*"
    ],
    "permissions": [
        "scripting",
        "activeTab"
    ],
    "commands": {
        "_execute_action": {
            "suggested_key": {
                "default": "Ctrl+Shift+B",
                "mac": "Command+B"
            }
        }
    },
    "icons": {
        "16": "images/favicon-16x16.png",
        "32": "images/favicon-32x32.png"
    }
}
